    "bcryptjs": "^2.4.3",
    "graphql": "^14.3.1",
    "graphql-tools": "^4.0.4",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-graphql": "^0.8.0",
    "koa-mount": "^4.0.0",
